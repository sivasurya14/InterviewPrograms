package employeeData;

public class Employee {

	        int id;
	        String name;
	        int age;
	        String gender;
	        String department;
	        double salary;
	        
	        
	        public Employee(int id, String name, int age, String gender, String department, double salary) {
	            this.id = id;
	            this.name = name;
	            this.age = age;
	            this.gender = gender;
	            this.department = department;
	            this.salary = salary;
	        }
	    }

	   
	 